{
  "name": "hh-blog-api",
  "version": "1.0.0",
  "description": "for nodejs",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.15.1",
    "egg-scripts": "^2.11.0",
    "egg-sequelize": "^6.0.0",
    "egg-validate": "^2.0.2",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.2.5"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0",
    "factory-girl": "^5.0.4",
    "sequelize-auto": "^0.7.6",
    "sequelize-cli": "^6.2.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --port=7001 --daemon --title=egg-server-hh-blog-api",
    "stop": "egg-scripts stop --title=egg-server-hh-blog-api",
    "dev": "egg-bin dev",
    "build":"npm install --production && tar -zcvf ../release.tgz .",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "create-model": "sequelize-auto -o ./app/model -h 121.4.115.182 -p 3306 -d blogdb -u reroot -x 666gjjhhh && node ./script/model-format.js"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
